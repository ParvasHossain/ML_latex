\newglossary{abbrev}{abs}{abo}{List of Abbreviations}

%% Accronyms/Abbreviations are placed here %%
% Call abbreviations as gls{EER}
%-----------------------------------------------------


\newglossaryentry{EER}{
    name = EER,
    description = Equal Error Rate,
    type = abbrev
}

\newglossaryentry{MFCC}{
    name = MFCC,
    description = Mel-Frequency Cepstral Coefficients,
    type = abbrev
}


